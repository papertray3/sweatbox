{
  "name": "@sweatbox/server",
  "version": "0.1.0",
  "description": "Server for sweatbox app",
  "main": "./dist/server/app.js",
  "scripts": {
    "electron": "electron",
    "rebuild": "electron-rebuild",
    "tsc": "tsc",
    "prebuild": "npm run genversion",
    "build": "tsc",
    "build:test": "npm run build",
    "prestart": "npm run build",
    "start": "NODE_ENV=development electron ./dist/server/app.js --rootDir ../client5/dist/public",
    "prestart:test": "npm run build:test",
    "start:test": "NODE_ENV=test node ./dist/server/app.js --rootDir ../client/dist/public",
    "clean": "rimraf dist/",
    "genversion": "genversion --es6 src/version.ts",
    "postinstall": "npm-run-all --parallel genversion"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/better-sqlite3": "^3.1.3",
    "@types/electron": "^1.6.10",
    "@types/image-size": "^0.0.29",
    "@types/log4js": "^2.3.5",
    "@types/mime": "^2.0.0",
    "@types/node": "^10.5.3",
    "electron": "^2.0.6",
    "electron-rebuild": "^1.8.2",
    "genversion": "^2.1.0",
    "npm-run-all": "^4.1.3",
    "rimraf": "^2.6.2",
    "typescript": "^2.9.2"
  },
  "dependencies": {
    "@papertray3/express-config": "git+ssh://git@github.com/papertray3/express-config.git",
    "@sweatbox-core/common": "^0.1.0",
    "@types/nconf": "0.0.37",
    "@types/yargs": "^11.1.1",
    "better-sqlite3": "^4.1.4",
    "image-size": "^0.6.3",
    "knuth-shuffle": "^1.0.8",
    "log4js": "^3.0.3",
    "mime": "^2.3.1",
    "rxjs": "^6.2.2"
  }
}
